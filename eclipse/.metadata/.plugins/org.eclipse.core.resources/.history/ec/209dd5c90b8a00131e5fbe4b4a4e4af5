	@Instance(value = "gaseousMC")
    public static GaseousMC instance;
    
    @SidedProxy(clientSide="DeBukkIt.Basis.Proxies.BasisClientProxy", serverSide="DeBukkIt.Basis.Proxies.BasisCommonProxy")
    public static GaseousMCCommonProxy proxy;
    
    @EventHandler
    public void preInit(FMLPreInitializationEvent event) {

    }
    
    @EventHandler
    public void load(FMLInitializationEvent event) {
            
    }
    
    @EventHandler
    public void postInit(FMLPostInitializationEvent event) {
    	
    }
}
