public class MachineBase extends Block {//TODO
	
	//Power Varibales
	public int power;
	public int pwrIn;
	public int pwrout;
	
	public MachineBase(int par1) {
		super(par1, Material.iron);
	}
	
	public TileEntity createTileEntity()
	{
		return TileentityMachine();
	}
	
}
